{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sample-organization-announcement-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sampleOrganizationAnnouncement/SampleOrganizationAnnouncementWebPart.js",
          "manifest": "./src/webparts/sampleOrganizationAnnouncement/SampleOrganizationAnnouncementWebPart.manifest.json"
        }
      ]
    },
    "important-organization-announcements-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/importantOrganizationAnnouncements/ImportantOrganizationAnnouncementsApplicationCustomizer.js",
          "manifest": "./src/extensions/importantOrganizationAnnouncements/ImportantOrganizationAnnouncementsApplicationCustomizer.manifest.json"
        }
      ]
    },
    "organization-announcements-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/organizationAnnouncements/OrganizationAnnouncementsAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/organizationAnnouncements/OrganizationAnnouncementsAdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SampleOrganizationAnnouncementWebPartStrings": "lib/webparts/sampleOrganizationAnnouncement/loc/{locale}.js",
    "ImportantOrganizationAnnouncementsApplicationCustomizerStrings": "lib/extensions/importantOrganizationAnnouncements/loc/{locale}.js",
    "OrganizationAnnouncementsAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/organizationAnnouncements/loc/{locale}.js"
  }
}
